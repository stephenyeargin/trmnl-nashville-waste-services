{%- comment -%}
Calculate today's date in user's local timezone using utc_offset
{%- endcomment -%}
{% assign now_ts = "now" | date: "%s" %}
{% assign today_ts = now_ts | plus: trmnl.user.utc_offset %}
{% assign today = today_ts | date: "%Y-%m-%d" %}
{% assign sorted_events = events | sort: "day" %}

{%- comment -%}
Find the next pickup day
{%- endcomment -%}
{% assign next_pickup_day = "" %}

{% for event in sorted_events %}
  {% if event.day >= today %}
    {% assign has_pickup = false %}
    {% for flag in event.flags %}
      {% if flag.opts.event_type == "pickup" %}
        {% assign has_pickup = true %}
      {% endif %}
    {% endfor %}
    {% if has_pickup and next_pickup_day == "" %}
      {% assign next_pickup_day = event.day %}
    {% endif %}
  {% endif %}
{% endfor %}

{%- comment -%}
Capture pickups for the next pickup day
{%- endcomment -%}
{% capture pickup_events %}
<div class="flex flex--center-y flex--center-x gap">
  {% for event in sorted_events %}
    {% if event.day == next_pickup_day %}
      {% assign pickup_type = "" %}
      {% for flag in event.flags %}
        {% if flag.opts.event_type == "pickup" %}
          {% assign pickup_type = flag.name %}
        {% endif %}
      {% endfor %}
      <div class="text--center">
        <img height="125" width="88"
        {% if pickup_type == "trash" %}
        src="data:image/svg+xml;base64,{{ svg_trash | base64_encode }}"
        {% elsif pickup_type == "recycle" %}
        src="data:image/svg+xml;base64,{{ svg_recycle | base64_encode }}"
        {% elsif pickup_type == "brush" %}
        src="data:image/svg+xml;base64,{{ svg_brush | base64_encode }}"
        {% else %}
        src="data:image/svg+xml;base64,{{ svg_unknown | base64_encode }}"
        {% endif %}
        alt="{{ pickup_type }}" />
        <div><span class="label label--small label--outline">{{ pickup_type | upcase }}{{ image_height }}</span></div>
      </div>
    {% endif %}
  {% endfor %}
</div>
{% endcapture %}

{%- comment -%}
Calculate days until next pickup
{%- endcomment -%}
{% if next_pickup_day != "" %}
  {% assign today_ts = today | date: "%s" %}
  {% assign next_pickup_ts = next_pickup_day | date: "%s" %}
  {% assign seconds_diff = next_pickup_ts | minus: today_ts %}
  {% assign days_until_pickup = seconds_diff | divided_by: 86400 %}
{% endif %}

{%- comment -%}
Capture upcoming holidays (still all future holidays)
{%- endcomment -%}
{% capture holiday_events %}
{% if holiday_events.length > 0 %}
<div class="flex gap gap--large">
  <div><span class="title title--small">Upcoming Holidays:</span></div>
  {% for event in sorted_events %}
    {% if event.is_holiday == 1 and event.day >= today %}
      {% assign holiday_flag = event.flags.first %}
      <div><span class="label label--small">{{ event.day | date: '%-m/%-d' }}: {{ holiday_flag.subject_hash["en-US"] }}</span></div>
    {% endif %}
  {% endfor %}
</div>
{% endif  %}
{% endcapture %}
